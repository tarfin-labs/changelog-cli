{
    "name": "tarfin-labs/changelog-cli",
    "description": "Changelog CLI.",
    "keywords": ["changelog", "cli"],
    "homepage": "https://tarfin.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tarfin-labs/changelog-cli/issues",
        "source": "https://github.com/tarfin-labs/changelog-cli"
    },
    "authors": [
        {
            "name": "Faruk Can",
            "email": "faruk.can@tarfin.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laminas/laminas-text": "^2.8",
        "laravel-zero/framework": "^8.0",
        "nunomaduro/laravel-console-menu": "^3.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["changelog-cli"]
}
