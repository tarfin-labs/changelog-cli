{
    "name": "tarfin-labs/changelog-cli",
    "description": "Changelog CLI.",
    "keywords": ["changelog", "cli"],
    "homepage": "https://tarfin.com",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/tarfin-labs/changelog-cli/issues",
        "source": "https://github.com/tarfin-labs/changelog-cli"
    },
    "authors": [
        {
            "name": "Faruk Can",
            "email": "faruk.can@tarfin.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4"
    },
    "require-dev": {
        "laminas/laminas-text": "^2.10",
        "laravel-zero/framework": "^12.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/laravel-console-menu": "^3.3",
        "phpunit/phpunit": "^12"
    },
    "autoload": {
        "psr-4": {
            "ChangelogCLI\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChangelogCLI\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/changelog-cli"
    ]
}
